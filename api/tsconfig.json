{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "outDir": ".vercel/output"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
